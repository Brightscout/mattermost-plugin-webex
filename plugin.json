{
    "id": "com.mattermost.webex",
    "name": "Webex",
    "description": "Webex audio and video conferencing plugin for Mattermost 5.12+.",
    "version": "0.0.1",
    "min_server_version": "5.12.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "settings": [
            {
                "key": "WebexURL-not-used-yet",
                "display_name": "Webex URL",
                "type": "text",
                "help_text": "The URL for a self-hosted private cloud or on-premise Webex server. For example, https://yourwebex.com. Leave blank if you're using Webex's vendor-hosted SaaS service.",
                "placeholder": "https://Webex.us"
            },
            {
                "key": "ClientID",
                "display_name": "Client ID",
                "type": "text",
                "help_text": "The Client ID generated by the Webex integration setup process."
            },
            {
                "key": "ClientSecret",
                "display_name": "Client Secret",
                "type": "text",
                "help_text": "The Client Secret generated by the Webex integration setup process."
            }
        ]
    }
}
